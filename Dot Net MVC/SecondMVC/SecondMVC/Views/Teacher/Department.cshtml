@using SecondMVC.Models
@model IEnumerable<SecondMVC.Models.Teachers>

@{
    Layout = null;
    ViewBag.Title = "Dept";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Deparments</title>
</head>
<body>
    <div>
        <h3>@ViewBag.Dept</h3>
        <ul>
            @foreach (Teachers tech in Model)
            {
                <li>
                    @Html.ActionLink(tech.Name, "Details", new { id = tech.Id })
                </li>
            }
        </ul>
        <p>
            @Html.ActionLink("Back to List", "Index")
        </p>
    </div>
</body>
</html>